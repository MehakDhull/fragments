{
  "name": "fragments",
  "private": true,
  "version": "0.7.1",
  "description": "Fragments back-end API",
  "main": "index.js",
  "scripts": {
    "test": "cross-env NODE_ENV=test jest -c jest.config.js --runInBand --",
    "test:watch": "jest -c jest.config.js --runInBand --watch --",
    "coverage": "jest -c jest.config.js --runInBand --coverage",
    "lint": "eslint \"./src/**/*.js\" \"tests/**/*.js\"",
    "format": "prettier --write \"src/**/*.js\" \"tests/**/*.js\"",
    "format:check": "prettier --check \"src/**/*.js\" \"tests/**/*.js\"",
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "debug": "node --inspect src/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/khmahida/fragments.git"
  },
  "author": "Mehak",
  "license": "UNLICENSED",
  "devDependencies": {
    "@eslint/js": "^9.31.0",
    "cross-env": "^7.0.3",
    "dotenv": "^17.2.0",
    "eslint": "^8.57.1",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.2.5",
    "supertest": "^7.1.3"
  },
  "dependencies": {
    "@aws-sdk/client-cognito-identity-provider": "^3.844.0",
    "amazon-cognito-identity-js": "^6.3.15",
    "aws-jwt-verify": "^5.1.0",
    "base-64": "^1.0.0",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "body-parser": "^2.2.0",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "docker": "^1.0.0",
    "express": "^4.21.2",
    "helmet": "^8.1.0",
    "http-auth": "^4.2.1",
    "http-auth-passport": "^1.0.7",
    "markdown-it": "^14.1.0",
    "morgan": "^1.10.0",
    "passport": "^0.7.0",
    "passport-azure-ad": "^4.3.5",
    "passport-cognito": "^0.2.2",
    "passport-http": "^0.3.0",
    "passport-http-bearer": "^1.0.1",
    "pino": "^8.21.0",
    "pino-http": "^8.6.1",
    "pino-pretty": "^10.3.1",
    "stoppable": "^1.1.0",
    "winston": "^3.17.0"
